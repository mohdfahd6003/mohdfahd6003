{
    "document": {
        "type": "APL",
        "version": "1.6",
        "license": "Copyright 2021 Amazon.com, Inc. or its affiliates. All Rights Reserved.\nSPDX-License-Identifier: LicenseRef-.amazon.com.-AmznSL-1.0\nLicensed under the Amazon Software License  http://aws.amazon.com/asl/",
        "settings": {},
        "theme": "light",
        "import": [
            {
                "name": "alexa-layouts",
                "version": "1.3.0"
            },
            {
                "name": "alexa-viewport-profiles",
                "version": "1.2.0"
            }
        ],
        "resources": [
            {
                "dimensions": {
                    "scrollViewHeight": "35vh"
                }
            },
            {
                "when": "${@viewportProfile == @tvLandscapeXLarge}",
                "dimensions": {
                    "scrollViewHeight": "20vh"
                }
            }
        ],
        "styles": {},
        "mainTemplate": {
            "parameters": [
                "payload"
            ],
            "items": [
                {
                    "items": [
                        {
                            "type": "AlexaBackground",
                            "backgroundColor": "#FFFFFF"
                        },
                        {
                            "items": [
                                {
                                    "item": {
                                        "imageSource": "https://dtzf6dz2qpnrm.cloudfront.net/images/Welcome_background.png",
                                        "imageScale": "best-fill",
                                        "type": "AlexaImage",
                                        "imageHeight": "77.5vh",
                                        "imageWidth": "63.984375vw",
                                        "accessibilityLabel": "American Heart Association home page image"
                                    },
                                    "borderWidth": "2px",
                                    "borderColor": "#C10E21",
                                    "type": "Frame",
                                    "width": "64.296875vw"
                                }
                            ],
                            "alignItems": "end",
                            "justifyContent": "center",
                            "type": "Container",
                            "paddingRight": "@marginHorizontal"
                        },
                        {
                            "items": [
                                {
                                    "item": [
                                        {
                                            "item": [
                                                {
                                                    "items": [
                                                        {
                                                            "source": "https://dtzf6dz2qpnrm.cloudfront.net/images/Aha_logo_2x.png",
                                                            "type": "Image",
                                                            "accessibilityLabel": "American Heart Association logo",
                                                            "width": "4.140625vw",
                                                            "height": "8.625vh"
                                                        }
                                                    ],
                                                    "alignItems": "center",
                                                    "wrap": "noWrap",
                                                    "type": "Container",
                                                    "accessibilityLabel": "American Heart Association logo container",
                                                    "width": "46.40625vw",
                                                    "padding": "@spacingSmall"
                                                },
                                                {
                                                    "item": [
                                                        {
                                                            "textAlign": "center",
                                                            "fontSize": "@fontSizeMedium",
                                                            "text": "The American Heart Association has been working to reduce disability and deaths caused by heart disease since 1924. Heart disease is the leading cause of death globally, and can cause numerous problems including heart attack or stroke.",
                                                            "textAlignVertical": "auto",
                                                            "fontFamily": "@fontFamilyRomanSans",
                                                            "fontWeight": "@fontWeightLight",
                                                            "color": "#222328",
                                                            "letterSpacing": "@letterSpacingNormal",
                                                            "lineHeight": "@lineHeightNormal",
                                                            "id": "introductionText",
                                                            "type": "Text",
                                                            "paddingBottom": "@spacingSmall",
                                                            "accessibilityLabel": "American Heart Association introduction text"
                                                        }
                                                    ],
                                                    "type": "ScrollView",
                                                    "height": "@scrollViewHeight",
                                                    "paddingLeft": "@spacingSmall",
                                                    "paddingRight": "@spacingSmall"
                                                },
                                                {
                                                    "backgroundColor": "#C10E21",
                                                    "type": "Frame",
                                                    "height": "1.25vh"
                                                }
                                            ],
                                            "justifyContent": "end",
                                            "type": "Container"
                                        }
                                    ],
                                    "backgroundColor": "#FFFFFF",
                                    "type": "Frame",
                                    "width": "46.40625vw",
                                    "shadowColor": "#00000029",
                                    "shadowRadius": "15",
                                    "shadowVerticalOffset": "0",
                                    "shadowHorizontalOffset": -3
                                }
                            ],
                            "position": "absolute",
                            "type": "Container",
                            "paddingLeft": "@marginHorizontal"
                        }
                    ],
                    "alignItems": "stretch",
                    "justifyContent": "center",
                    "type": "Container"
                }
            ]
        },
        "layouts": {},
        "onMount": {
            "type": "AnimateItem",
            "easing": "ease-in-out",
            "duration": 800,
            "componentId": "introductionText",
            "value": [
                {
                    "property": "opacity",
                    "to": 1
                },
                {
                    "property": "transform",
                    "from": [
                        {
                            "translateX": -500
                        }
                    ],
                    "to": [
                        {
                            "translateX": 0
                        }
                    ]
                }
            ]
        }
    },
    "datasources": {},
    "sources": {}
}