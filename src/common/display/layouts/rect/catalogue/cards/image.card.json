{
    "item": [
        {
            "imageSource": "${data.imageSrc}",
            "type": "AlexaImage",
            "id": "rect.card.image",
            "imageWidth": "@imageWidth",
            "imageHeight": "@imageHeight",
            "imageScale": "best-fill",
            "align": "start",
            "borderRadius": "4"
        }
    ],
    "shrink": 1,
    "alignItems": "center",
    "justifyContent": "start",
    "alignSelf": "center",
    "type": "Container",
    "width": "100%",
    "id": "rect.card.imageContainer",
    "paddingLeft": "@gridImagePadding",
    "paddingRight": "@gridImagePadding"
}
