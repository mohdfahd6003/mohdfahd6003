{  "logoContainer": {
        "values": [
            {
                "when": "${@viewportShape == @viewportShapeRound}",
                "justifyContent": "center"
            },
            {
                "when": "${@viewportShape == @viewportShapeRectangle}",
                "justifyContent": "end"
            }
        ]
    },
    "image.header":{
        "values":[
            {
                "imageAlignment": "center",
                "imageScale": "best-fit",
                "imageAspectRatio": "widescreen"
        

            }
        ]
    },
    "mainText.explain":{
        "values":[
            {
                "fontFamily": "@fontFamilyRomanSans",
                "fontSize": "4vh",
                "lineHeight": "@lineHeightNormal",
                "letterSpacing": "@letterSpacingNormal",
                "color": "#FFFFFF",
                "fontWeight": "400",
                "textAlign": "left"    

            }
        ]
    },
    "title.explain":{
        "values":[
            {
            "fontFamily": "@fontFamilyRomanSans",
            "fontSize": "6vh",
            "lineHeight": "@lineHeightNormal",
            "letterSpacing": "@letterSpacingNormal",
            "color": "#FFFFFF",
            "maxLines": 2,
            "fontWeight": "500",
            "paddingLeft": "6.25vw",
            "textAlign": "left"
            }
        ]
    },
    "footerText": {
        "values": [
            {
                "textAlign": "left",
                "fontStyle": "italic",
                "fontWeight": "400",
                "textAlignVertical": "center",
                "opacity": 1,
                "color": "#222328",
                "height": "12.5vh",
                "width": "100vw",
                "fontFamily": "Bookerly",
                "fontSize": "@fontSizeMedium",
                "lineHeight": 1.5
            }
        ]
    },
    "welcomeText":{
        "values":[
           {
              "textAlign":"center",
              "textAlignVertical":"center",
              "color":"#222328",
              "fontSize":"@fontSizeXLarge",
              "lineHeight":"@lineHeightNormal",
              "letterSpacing":"@letterSpacingNormal"
           }
        ]
     },
     "rect.welcome.footer":{
        "values":[
           {
              "textAlign":"left",
              "fontStyle":"italic",
              "fontWeight":"400",
              "textAlignVertical":"center",
              "opacity":1,
              "color":"#222328",
              "height":"12.5vh",
              "width":"100vw",
              "fontFamily":"Bookerly",
              "fontSize":"@fontSizeMedium",
              "letterSpacing":"@letterSpacingNormal",
              "lineHeight":"1.5"
           }
        ]
     },
     "imageContainer":{
        "values":[
           {
              "justifyContent":"center",
              "left":"36vw"
           }
        ]
     },
     "image":{
        "values":[
           {
              "opacity":1,
              "align":"right",
              "scale":"best-fill",
              "shadowColor":"#000029",
              "shadowRadius":"25",
              "shadowVerticalOffset":"3",
              "shadowHorizontalOffset":"0"
           }
        ]
     },
     "welcomeTextFrame":{
        "values":[
           {
              "shadowColor":"#000029",
              "shadowVerticalOffset":"0",
              "shadowRadius":"15",
              "shadowHorizontalOffset":"-3"
           }
        ]
     },
     "cardFrame": {
        "values": [
            {
                "backgroundColor": "#FFFFFF",
                "borderRadius": 4,
                "shadowVerticalOffset": 3,
                "shadowHorizontalOffset": 3,
                "shadowColor": "#00000029",
                "shadowRadius": 10
            },
            {
                "when": "${state.pressed}",
                "backgroundColor": "#fcfcfa"
            }
        ]
    },
    "carousel": {
        "values": [
            {
                "width": "100%",
                "snap": "center",
                "height": "@cardHeight",
                "paddingTop": "@spacingSmall",
                "paddingBottom": "@spacingSmall",
                "paddingLeft": "@spacingMedium",
                "paddingRight": "@spacingMedium"
            }
        ]
    }
  
}